[user]
  name = sara-samy
  email = intermsofart@hotmail.com
[core]
  editor = nvim
[merge]
  commit = no
[push]
  default = current
[alias]
  log = log --graph --pretty=tformat:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --decorate=full 
  unpushed = !GIT_CURRENT_BRANCH=$(git name-rev --name-only HEAD) && git log origin/$GIT_CURRENT_BRANCH..$GIT_CURRENT_BRANCH --oneline
  unstage = restore --staged
  uncommit = reset --soft HEAD~1
  checkout-remote-branch = checkout --track
  delete-remote-branch = push origin --delete
  delete-local-branch = branch -d
